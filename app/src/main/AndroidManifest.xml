<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="adventuregame1">

    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="adventuregame1.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="adventuregame1.NormalActivity"
            android:parentActivityName="adventuregame1.MainActivity">


        </activity>
        <activity android:name="adventuregame1.HardActivity"
            android:parentActivityName="adventuregame1.HardActivity">

        </activity>
    </application>

</manifest>
